{
  "extends": "airbnb",
  "rules": {
    "semi": [2, "never"],
    "no-new": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "no-alert": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "window": true,
    "document": true,
    "Turbolinks": true
  }
}
